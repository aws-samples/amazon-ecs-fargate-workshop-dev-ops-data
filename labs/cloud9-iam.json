                "kafka:*", 
                "rds:*", 
                "ecr:*", 
                "ecs:*", 
                "ec2:*", 
                "servicediscovery:*", 
                "lambda:*", 
                "ssm:*", 
                "codepipeline:*", 
                "codecommit:*", 
                "codebuild:*", 
                "cloudwatch:*", 
                "transfer:*", 
                "elasticloadbalancing:*",
                "cloudformation:*",
